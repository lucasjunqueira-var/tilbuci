<project>

	<!-- app setup -->
	<meta title="TilBuci" package="br.com.tilbuci" version="1.0.0" company="VAR" />
	<app main="Main" path="Export" file="TilBuci" preloader="Preloader" />
	<window background="#000000" />
	
	<!-- code source -->
	<source path="Source" />
	
	<!-- used libraries -->
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	<haxelib name="feathersui" />
	<haxelib name="crypto" />
	<haxelib name="moonshine-feathersui-text-editor" />
	<haxelib name="hscript" />

	<!-- renderer -->
	<define name="dom" if="html5" />

	<!-- javascript externs -->
	<dependency path="Externs/browser.js" embed="true" />
	<dependency path="Externs/upload.js" embed="true" />
	<dependency path="Externs/embedcontent.js" embed="true" />
	<dependency path="Externs/overlayplugin.js" embed="true" />
	
